@model string
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container" ng-controller="questionController">
    <div class="row">
        <div class="col-md-6">
            <table class="table table-condensed table-hover">
                <tr>
                    <th>Question</th>
                    <th>Answer</th>
                </tr>
                <tr ng-repeat="question in questions">
                    <td>
                        {{question.text}}
                    </td>
                </tr>
            </table>
        </div>
    </div>
</div>





